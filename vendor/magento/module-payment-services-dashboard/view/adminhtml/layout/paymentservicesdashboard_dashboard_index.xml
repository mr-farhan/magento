<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="header">
            <referenceBlock name="tracking">
                <arguments>
                    <argument name="tracking_url" xsi:type="string">//assets.adobedtm.com/launch-EN30eb7ffa064444f1b8b0368ef38fd3a9.min.js</argument>
                </arguments>
            </referenceBlock>
        </referenceContainer>
        <referenceBlock name="page.title">
            <action method="setPageTitle">
                <argument name="title" translate="true" xsi:type="string">Payment Services</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\PaymentServicesDashboard\Block\Adminhtml\Index" name="paymentsdashboard.index" template="Magento_PaymentServicesDashboard::adminhtml/index.phtml"/>
        </referenceContainer>
    </body>
</page>
